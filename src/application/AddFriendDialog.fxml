<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="450.0" prefWidth="350.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" 
				fx:controller="ClientController.AddFriendDialogController">
   <children>
      <TextField fx:id="searchFriend" layoutY="-1.0" prefHeight="39.0" prefWidth="261.0" promptText="찾을 친구 아이디 검색" />
      <Button fx:id="btnSearch" layoutX="261.0" mnemonicParsing="false" prefHeight="39.0" prefWidth="90.0" text="검색">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
      <ListView fx:id="searchFriendList" layoutY="38.0" prefHeight="412.0" prefWidth="350.0" />
      <Button fx:id="btnAddFriend" layoutX="247.0" layoutY="342.0" mnemonicParsing="false" prefHeight="39.0" prefWidth="90.0" text="친구추가">
         <font>
            <Font size="16.0" />
         </font>
      </Button>
      <Button fx:id="btnCancel" layoutX="247.0" layoutY="397.0" mnemonicParsing="false" prefHeight="39.0" prefWidth="90.0" text="취소">
         <font>
            <Font size="16.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
